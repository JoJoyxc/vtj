    "build:pre": "vue-tsc && cross-env ENV_TYPE=pre vite build",
    "build:prod": "vue-tsc && cross-env ENV_TYPE=live vite build",
    "build:ext": "cross-env Extension=true vite build",
    "preview": "vite preview"
