    "@vtj/cli": "workspace:~",
    "@vtj/core": "workspace:~",
    "@vtj/pro": "workspace:~",
    "@vtj/web": "workspace:~",
    "vue": "~3.5.5",
    "vue-router": "~4.5.0"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/vtj-block-plugin.mjs"
    }
  },
  "main": "./dist/vtj-block-plugin.umd.js",
  "module": "./dist/vtj-block-plugin.mjs",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "gitHead": "0b470d76f584714cd96bf684bf66c720c2661df1",
